{
    "name": "production-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --env port=3000",
        "start:dev": "concurrently \"npm start\" \"npm run start:dev:server\"",
        "start:dev:server": "node ./json-server/index.js",
        "build:prod": "webpack --env mode=production apiUrl=https://produ-json-server.vercel.app/",
        "build:dev": "webpack --env mode=development",
        "webpack": "webpack",
        "lint:ts": "./node_modules/.bin/eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": "./node_modules/.bin/eslint \"**/*.{ts,tsx,js}\" --fix",
        "lint:css": "npx stylelint \"**/*.scss\"",
        "lint:css:fix": "npx stylelint \"**/*.scss\" --fix",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:ui": "npx loki test",
        "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
        "test:ui:ok": "npx loki approve",
        "test:ui:json": "node scripts/generate-visual-json-report.js",
        "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
        "storybook": "storybook dev -p 6006 -c ./config/storybook",
        "build-storybook": "storybook build -c ./config/storybook",
        "prepare": "husky install",
        "generate:slice": "node ./scripts/createSlice/index.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "@storybook/addon-essentials": "^7.6.16",
        "@storybook/addon-interactions": "^7.6.16",
        "@storybook/addon-links": "^7.6.17",
        "@storybook/addon-onboarding": "^1.0.11",
        "@storybook/blocks": "^7.6.16",
        "@storybook/react": "^7.6.16",
        "@storybook/react-webpack5": "^7.6.16",
        "@storybook/test": "^7.6.16",
        "@svgr/webpack": "^6.2.1",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "@testing-library/user-event": "^14.3.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.4.1",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "babel-loader": "^8.2.3",
        "babel-plugin-i18next-extract": "^0.8.3",
        "copy-webpack-plugin": "^10.2.4",
        "cors": "^2.8.5",
        "css-loader": "^6.6.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-i18next": "^5.1.2",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.2",
        "eslint-plugin-react-hooks": "^4.3.0",
