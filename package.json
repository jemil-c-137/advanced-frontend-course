        "eslint-plugin-the-fsd-path-plugin": "^0.0.5",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.5.1",
        "jest-fetch-mock": "^3.0.3",
        "jest-html-reporters": "^3.1.7",
        "json-server": "^0.17.0",
        "loki": "^0.34.0",
        "mini-css-extract-plugin": "^2.5.3",
        "react-refresh": "^0.14.0",
        "reg-cli": "^0.17.7",
        "sass": "^1.49.9",
