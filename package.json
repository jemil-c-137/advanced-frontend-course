        "stylelint": "^14.5.3",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard-scss": "^3.0.0",
        "stylelint-order": "^6.0.3",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5",
